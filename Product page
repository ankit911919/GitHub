    <p>You have selected multiple sizes of the same style.<br>Please look at the <a style="color: #000000; text-decoration: underline;" href="https://development1.masongarments.com/frequently-asked-questions/shopping-information/size-chart.html"><u>size chart</u></a> to find the size that fits your feet perfectly.<br><br>By doing this, you save yourself future returning costs, reduce CO2 emission and prevent waste of packaging parcels.<br><br><em>Thank you in advance!</em><br><br><strong>Team Mason Garments</strong><br><br></p></div>
<script>
require(['jquery', 'mage/url', 'Magento_Ui/js/modal/modal', 'js/bootstrap.bundle.min', 'js/equalheight', 'js/placeholders.min', 'mage/mage'], function ($, murl, modal) {
    $(document).ready(function(){

        // clear button stays black start

        $(function(){
            $(".filter-overly-bg .filter-footer .button-row .clear").click(function() {
             $('.filter-overly-bg .filter-footer .button-row .clear').addClass("make_me_white");
             setTimeout(RemoveClass, 1000);
            });
            function RemoveClass() {
            $('.filter-overly-bg .filter-footer .button-row .clear').removeClass("make_me_white");
            }
        }); 

        // clear button stays black start

        // when you hover a toggle show its dropdown menu
        $(".navbar .dropdown-toggle").mouseover(function () {
            $(this).parent().toggleClass("show");
            $(this).parent().find(".dropdown-menu").toggleClass("show");
        });
        // hide the menu when the mouse leaves the dropdown
        $(".navbar .dropdown-menu" ).mouseleave(function() {
            $(this).removeClass("show");
        });
        // breakpoint and up
        $(window).resize(function(){
            if($(window).width() >= 980){
                // when you hover a toggle show its dropdown menu
                $(".navbar .dropdown-toggle").mouseover(function () {
                   $(this).parent().toggleClass("show");
                   $(this).parent().find(".dropdown-menu").toggleClass("show");
                });
                // hide the menu when the mouse leaves the dropdown
                $( ".navbar .dropdown-menu" ).mouseleave(function() {
                  $(this).removeClass("show");
                });
                // do something here
            }
        });
        $(window).scroll(function(){
            var scroll = $(window).scrollTop();
            if (scroll >= 1) {
                $(".header").addClass("darkHeader");
            } else {
                $(".header").removeClass("darkHeader");
            }
        });
        //equalheight Start
        $(window).on("load", function(){
            equalheight('.home-product-bar-section .product-box');
        });
        $(window).resize(function(){
            equalheight('.home-product-bar-section .product-box');
        });
        //equalheight End
        $(".more-button").click(function() {
            if($(".more-content").css("display") == "none") {
                $(this).addClass("up");
            } else {
                $(this).removeClass("up");
            }

            $(".more-content").slideToggle("fast");
        });
        var removeClass = true;
        $(".header-search-buttons a").click(function() {
            if(!$(".header-nav-search").hasClass("_active")) {
                $(".header-nav-search").addClass("_active");
                $(".header-search-buttons a").addClass("_active");
                removeClass = false;
            } else {
                $(".header-nav-search").removeClass("_active");
                $(".header-search-buttons a").removeClass("_active");
                removeClass = true;
            }    
        });
        $("#header-nav-search-input, .header-nav-search-submit").click(function() {
            $(".header-nav-search").addClass("_active");
            $(".header-search-buttons a").addClass("_active");
            removeClass = false;
        });
        $("html").click(function() {
            removeClass = true;
        });
        $(".filter-overly-bg .button-row button.close").click(function() {
            $(".filter-overly-bg").removeClass("show");
        });
        var removeClass_side_pennel = true;
        $(".action.showcart, .mage-dropdown-dialog-1").click(function() {
            $(".minicart-sidebar").addClass("active");
            $(".minicart-sidebar a.action.showcart").addClass("active");
            //$("html").addClass("cart-open");
            removeClass_side_pennel = false;
        });
        $("html, .close-minicart").click(function() {
            if (removeClass_side_pennel) {
                $(".minicart-sidebar").removeClass("active");
                $(".minicart-sidebar a.action.showcart").removeClass("active");
                //$("html").removeClass("cart-open");
            }
            removeClass_side_pennel = true; 
        });
    	$('#frm_register').mage('validation',
		{
            submitHandler: function(form) {
                $.ajax({
                    url: $('#frm_register').attr("action"),
                    data: $('#frm_register').serialize(),
                    type: 'POST',
                    dataType: 'html',
                    showLoader: true,
                    success: function(data, status, xhr) {
                        $('#frm_register_msg').html(data).show();
                        setTimeout(function(){
                            $('#frm_register_msg').html('').hide();
                        },3000);

                        if(data == '<span class="success">Success!</span>') {
                            window.location.href = "https://development.masongarments.com/customer/account/";
                        }
                    }
                });
            }
        });
        $('#frm_login').mage('validation',
		{
            submitHandler: function(form) {
                $.ajax({
                    url: $('#frm_login').attr("action"),
                    data: $('#frm_login').serialize(),
                    type: 'POST',
                    dataType: 'html',
                    showLoader: true,
                    success: function(data, status, xhr) {
                        $('#frm_login_msg').html(data).show();
                        setTimeout(function(){
                            $('#frm_login_msg').html('').hide();
                        },3000);

                        if(data == '<span class="success">Success!</span>') {
                            window.location.href = "https://development.masongarments.com/footwear.html?mg_color=60?login=" + $.now();
                        }
                    }
                });
            }
        });
        $('#frm_forgot_password').mage('validation',
		{
            submitHandler: function(form) {
                $.ajax({
                    url: $('#frm_forgot_password').attr("action"),
                    data: $('#frm_forgot_password').serialize(),
                    type: 'POST',
                    dataType: 'html',
                    showLoader: true,
                    success: function(data, status, xhr) {
                        $('#frm_forgot_password_msg').html(data).show();
                        setTimeout(function(){
                            $('#frm_forgot_password_msg').html('').hide();
                        },3000);
                    }
                });
            }
        });
        $('#frm_newsletter').mage('validation',
		{
            submitHandler: function(form) {
                $.ajax({
                    url: $('#frm_newsletter').attr("action"),
                    data: $('#frm_newsletter').serialize(),
                    type: 'POST',
                    dataType: 'html',
                    showLoader: true,
                    success: function(data, status, xhr) {
                        $('input[id="newsletter"]').val("");
                        $('#frm_newsletter_msg').html(data);
                        $('.btn_fnm').trigger('click');
                    }
                });
            }
        });
        $('#close-minicart').click(function(){
            $('#btn-minicart-close').trigger('click');
        });
        $('.language-span').html($('li[id="curr_lang"]').attr("lang"));
        $('#btn_clear_filter').click(function(){
            $('#clear_all_filter').trigger('click');
        });
        $('#view_all_products').click(function(){
            $('select[id="limiter"]').val('all').trigger('change');
        });

        var ajaxLoading = false;
        $(window).scroll(function() {
            if($(window).scrollTop() + $(window).height() > $(document).height() - 100) {
                if($('li[class="item pages-item-next"]').length && ajaxLoading == false) {
                    var nextUrl = $('li[class="item pages-item-next"]').find('a[title="Next"]').attr("href");
                    $.ajax({
                        url: nextUrl,
                        showLoader: true,
                        dataType: "json",
                        beforeSend: function(xhr) {
                            ajaxLoading = true;
                        }
                    }).done(function(response) {
                        ajaxLoading = false;
                        $('div[id="area_product_list"]').append($(response.products).find('div[id="area_product_list"]').html());
                        $('div[id="area_product_toolbar"]').html($(response.products).find('div[id="area_product_toolbar"]').html());
                        $(window.mNavigationConstants.AJAX_PRODUCT_LIST_WRAPPER_ID).trigger('contentUpdated');
                        $(window.mNavigationConstants.AJAX_PRODUCT_LIST_WRAPPER_ID).trigger('amscroll_refresh');
                    });
                }
            }        
        });

        $('#see_filter, #close_filter').click(function(){
            setTimeout(function(){
                if($('#filter-overly-bg').hasClass("show")){
                    $('#cat_title').hide();
                    $('#cat_title_filter').show();
                } else {
                    $('#cat_title_filter').hide();
                    $('#cat_title').show();
                }
            },500);
        });

        var is_login = "";
        if(!is_login) {
            $('#footer_my_account, .add_to_favorites').click(function(){
                $('.social-login').trigger('click');
                return false;
            });
        }

        $('#apply_filter').click(function(){
            $('#close_filter').trigger('click');
        });

        $('li[id^="size_"][class="selected"]').each(function() {
            var size = $(this).attr('id').split('_');
            size = size[1];

            if($('div[class="product-box"]').length == $('li[id^="size_'+size+'"][class="selected"]').length) {
                $('div[class="size-col"]').find('a[aria-label="'+size+'"]').parent("div").remove();
            }    
        });

        $('#btn_on_foot, #res_btn_on_foot').click(function(){
            $('img[class="prd_main_img"]').each(function(){
                var curr_img = $(this).attr('src');
                var on_foot = $(this).attr('on_foot');

                $(this).attr('src', on_foot);
                $(this).attr('on_foot', curr_img);
            });
        });

        $( window ).resize(function() {
            $('li[class^="ninjamenus-nav-item level1"]').show();
            $('a[class^="ninjamenus-nav-link"][href="#"]').show();
        });

        $('a[href*="customer/account/logout"]').click(function(){
            $('#frm_newsletter_msg').html('<span class="success">You are logged out successfully.</span>');
            $('.btn_fnm').trigger('click');
        });

        $('a[href*="customer/account/login"]').click(function(){
            $('.social-login').trigger('click');
            return false;
        });

        $(document).on("ajaxComplete", function(){
            $('body').trigger('processStop');
        });

        $('#frm_authenticity').mage('validation',
        {
            submitHandler: function(form) {
                $.ajax({
                    url: $('#frm_authenticity').attr("action"),
                    data: $('#frm_authenticity').serialize(),
                    type: 'POST',
                    dataType: 'html',
                    showLoader: true,
                    success: function(data, status, xhr) {
                        $('#frm_authenticity_msg').html(data);
                    }
                });
            }
        });

        //for checkout page TigPostNL
        setTimeout(function(){
            console.log("TIG error");
            var tig_error = '<span class="tig-postcode-validation-message">Sorry, we could not find the address on the given zip code and house number combination.</span>';
            $('div[name="shippingAddress.street.2"]').after(tig_error);
            $('div[name="billingAddressshared.street.2"]').after(tig_error);
            //fix for IE loader issue
            if($('.loading-mask').length) {
                $('.loading-mask').remove();
                console.log("IE loader removed");
            }
            //end
        }, 10000);
        $(document).on('change', '[name="country_id"]', function () {
            var parent_name = $(this).parent('div').parent('div').attr('name');
            if(parent_name == 'shippingAddress.country_id') {
                var parent_id = $('form[id="co-shipping-form"]');
            }
            if(parent_name == 'billingAddressshared.country_id') {
                var parent_id = $('div[class="billing-address-form"]');
            }

            var country_id = $(this).val();
            if(country_id == 'NL') {
                var postcode = parent_id.find('input[name="postcode"]').val();
                var huisnummer = parent_id.find('input[name="street[1]"]').val();
                if(postcode != "" && huisnummer != "") {
                    $.ajax({
                        url: murl.build('mason/order/tigPostNL'),
                        data: 'postcode='+postcode+'&huisnummer='+huisnummer+'',
                        type: 'POST',
                        dataType: 'json',
                        showLoader: true,
                        success: function(resdata, status, xhr) {
                            if(resdata.success == true) {
                                parent_id.find('input[name="street[0]"]').val(resdata.straatnaam).attr('readonly','readonly').trigger("keyup");
                                parent_id.find('input[name="city"]').val(resdata.woonplaats).attr('readonly','readonly').trigger("keyup");
                                parent_id.find('.tig-postcode-validation-message').hide();
                            } else {
                                parent_id.find('input[name="street[0]"]').attr('readonly','readonly').val('').trigger("keyup");
                                parent_id.find('input[name="city"]').attr('readonly','readonly').val('').trigger("keyup");
                                parent_id.find('.tig-postcode-validation-message').show();
                            }
                        }
                    });
                } else {
                    parent_id.find('.tig-postcode-validation-message').hide();
                }
            } else {
                parent_id.find('input[name="street[0]"]').removeAttr('readonly').trigger("keyup");
                parent_id.find('input[name="city"]').removeAttr('readonly').trigger("keyup");
                parent_id.find('.tig-postcode-validation-message').hide();
            }
        });
        $(document).on('blur', '[name="postcode"]', function () {
            var parent_name = $(this).parent('div').parent('div').attr('name');
            if(parent_name == "shippingAddress.postcode") {
                $('form[id="co-shipping-form"]').find('select[name="country_id"]').trigger('change');
            }
            if(parent_name == "billingAddressshared.postcode") {
                $('div[class="billing-address-form"]').find('select[name="country_id"]').trigger('change');
            }
        });
        $(document).on('blur', '[name="street[1]"]', function () {
            var parent_name = $(this).parent('div').parent('div').attr('name');
            if(parent_name == "shippingAddress.street.1") {
                $('form[id="co-shipping-form"]').find('select[name="country_id"]').trigger('change');
            }
            if(parent_name == "billingAddressshared.street.1") {
                $('div[class="billing-address-form"]').find('select[name="country_id"]').trigger('change');
            }
        });
        //end

        $(document).on('click', '[class="action primary checkout"]', function (e) {
            var sku_Arr = [];
            $('input[class="item-qty cart-item-qty"]').each(function() {
                var sku = $(this).attr("data-cart-item-id").split("-");
                var size = eval(sku.pop());
                sku = sku.join("-");
                if(!sku_Arr[sku]) {
                    sku_Arr[sku] = [size];
                } else {
                    var small_size = size - 1;
                    var large_size = size + 1;
                    if(sku_Arr[sku].indexOf(small_size) >= 0 || sku_Arr[sku].indexOf(large_size) >= 0) {
                        var is_validate = $('#checkout-same-shoe').attr("validate");
                        if(is_validate == "0") {
                            e.preventDefault();
                            $('#checkout-same-shoe').attr("validate", "1");
                            options = {
                                'type': 'popup',
                                'modalClass': 'checkout-same-shoe-modal',
                                'responsive': true,
                                'innerScroll': true,
                                'buttons': [
                                    {
                                        text: 'Close',
                                        class: 'action secondary action-hide-popup',
                                        click: function () {
                                            this.closeModal();
                                        }
                                    }
                                ],
                                'autoOpen': true
                            };
                            modal(options, $('#checkout-same-shoe'));
                            return false;
                        }
                    }
                    sku_Arr[sku].push(size);
                }
            });
        });
    });
});
</script>
