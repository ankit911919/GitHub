<script>
require([
    'jquery',
    'mage/url'
], function ($, mageUrl) {
    
    function getUserData()
    {
        var sectionUrl = 'https://development.masongarments.com/customer/section/load';
        
        $.ajax({
            url: sectionUrl,
            data: {
                sections: "apptrian_facebook_pixel_matching_section"
            },
            type: "GET",
            dataType : "json"
        })
        .done(function( json ) {
            var result = json.apptrian_facebook_pixel_matching_section.matching_data;
        });
    }
    
    getUserData();
    
});
