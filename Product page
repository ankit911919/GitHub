<script>
    function handleFiltersNavPositions() {
        var filtersConfig = {"manufacturer":"sidebar","color":"sidebar","mptablerate_shipping_group":"sidebar","mg_size":"horizontal","mg_laces":"sidebar","mg_amount":"sidebar","mg_style":"horizontal","mg_color":"horizontal","mg_fabric":"horizontal"};

        document.querySelectorAll('.sidebar [data-mst-nav-filter]').forEach(function (filterOptions) {
            var attributeCode = filterOptions.getAttribute('data-mst-nav-filter').replace(/A\d{6}A/, '');

            if (filtersConfig.hasOwnProperty(attributeCode)) {
                filterOptions.closest('.filter-options-item').setAttribute('data-nav-position', filtersConfig[attributeCode]);
            }
        }.bind(this))
    }

    document.addEventListener('DOMContentLoaded', handleFiltersNavPositions);
</script>

